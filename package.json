{
  "name": "ts-express-study",
  "version": "1.0.0",
  "main": "app.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "dev": "cross-env NODE_ENV=local nodemon --watch \"src/**/*.ts\" --exec \"ts-node\" src/app.ts",
    "start:dev": "cross-env NODE_ENV=dev node build/app.ts",
    "start:prod": "cross-env NODE_ENV=prod node build/app.js"
  },
  "devDependencies": {
    "@types/express": "^4.17.15",
    "@types/express-serve-static-core": "^4.17.31",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.16",
    "express": "^4.18.2",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "morgan": "^1.10.0"
  }
}
